<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.omarf.firstprojectdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/quizhunter"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.User.PlayActivity" />
        <activity android:name=".Activity.Admin.ListOfQuestionAdminActivity" />
        <activity android:name=".Activity.Admin.AddEditQuestionAdminActivity" />
        <activity android:name=".Activity.Admin.AdminPanelActivity">

        </activity>

        <service android:name=".SyncService" />

        <activity android:name=".Activity.User.WrongAnswersActivity" />
        <activity android:name=".Activity.User.ResultActivity" />
        <activity android:name=".Activity.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.User.UserPanelActivity"></activity>
    </application>

</manifest>